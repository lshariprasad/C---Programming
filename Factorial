#include<stdio.h>
int main()
{
    int i,fact = 1 ,n;
    
    printf("Enter The Number Of Rows : ");
    scanf("%d",&n);
    printf("\n");
    
    for(i=1;i<=n;i++)
    {
           fact *=i   ;                                                             
        
    }                                                                          printf("Factorial Of %d = %d ",n,fact);
    return 0; 
}
